@page "/test"
@using Environment = System.Environment
@inherits BasePage

<MudExCaptureChild OnComponentCaptured="@OnComponentCaptured" >
    <MudButton Color="Color.Secondary" Variant="Variant.Filled" > Hello </MudButton>
</MudExCaptureChild>


<DemoComponent PageType="@GetType()">
    <MudButton Color="Color.Info" Variant="Variant.Outlined" OnClick="Callback"> Demo Button </MudButton>

</DemoComponent>





@code {


    private void OnComponentCaptured(object obj)
    {
        Console.WriteLine($"Captured component of type: {obj.GetType().FullName}");
    }

    private void Callback()
    {
        string yamlContent = string.Join(Environment.NewLine, YamlLocalizerExtensions.NotFoundKeys.Select(s => $"{s}: {s}"));
        Console.WriteLine("YAML Content:");
        Console.WriteLine(yamlContent);
    }

}